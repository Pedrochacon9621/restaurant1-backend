{
    "builds": [
        {
            "src": "restaurant1/wsgi.py", 
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb", 
                "runtime": "python3.12" 
            } 
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "restaurant1/wsgi.py"
        }
    ],
    "installCommand": "pip install -r requirements.txt",
    "buildCommand": "./build.sh"
}